{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "base": "#1e1e2e",
    "blue": "#8aadf4",
    "crust": "#11111b",
    "flamingo": "#f0c6c6",
    "green": "#a6da95",
    "lavender": "#b7bdf8",
    "mantle": "#181825",
    "maroon": "#ee99a0",
    "mauve": "#c6a0f6",
    "overlay0": "#6e738d",
    "overlay1": "#8087a2",
    "overlay2": "#939ab7",
    "peach": "#f5a97f",
    "pink": "#f5bde6",
    "red": "#ed8796",
    "rosewater": "#f4dbd6",
    "sapphire": "#7dc4e4",
    "sky": "#91d7e3",
    "subtext0": "#a6adc8",
    "subtext1": "#bac2de",
    "surface0": "#313244",
    "surface1": "#45475a",
    "surface2": "#585b70",
    "teal": "#8bd5ca",
    "text": "#cad3f5",
    "yellow": "#eed49f"
  },
  "terminal_background": "p:mantle",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "template": "{{ if .SSHSession }}\ueb39{{ end }}{{ if .Root }}\uf511{{ end }}",
          "foreground": "p:teal",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "mapped_shell_names": {
              "bash": "\uebca",
              "cmd": "\ueb4c",
              "fish": "\uee41",
              "powershell": "\uebc7",
              "pwsh": "\uebc7",
              "zsh": "\udb83\udc36\ue760"
            }
          },
          "foreground": "p:teal",
          "type": "shell",
          "style": "plain"
        },
        {
          "type": "os",
          "template": "{{ if .WSL }}[WSL @ {{ .Icon }}]{{ end }}",
          "foreground": "p:sapphire",
          "style": "plain"
        },
        {
          "template": "[{{ .UserName }}@{{ .HostName }}]",
          "foreground": "p:teal",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "playing_icon": "\uf1bc"
          },
          "template": "[{{ if ne .Status \"stopped\" }}{{ .Icon }} {{ .Track }} ~ {{ .Artist }}{{ end }}]",
          "foreground": "p:green",
          "type": "spotify",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "time_format": "15:04:05"
          },
          "template": "[{{ .CurrentDate | date .Format }}]",
          "foreground": "p:peach",
          "type": "time",
          "style": "plain"
        },
        {
          "properties": {
            "charged_icon": "\udb80\udc84",
            "charging_icon": "\udb80\udc89",
            "discharging_icon": "\udb80\udc7c",
            "not_charging_icon": "\udb81\udea5"
          },
          "template": "{{ if not .Error }}[{{ .Icon }} {{ .Percentage }}%]{{ end }}",
          "foreground": "p:yellow",
          "type": "battery",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "folder_separator_icon": "/",
            "style": "full"
          },
          "template": "[{{ .Path }}]",
          "foreground": "p:mauve",
          "type": "path",
          "style": "plain"
        },
        {
          "template": "[{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if .AheadBehind.Ahead }} ↑{{ .AheadBehind.Ahead }}{{ end }}{{ if .AheadBehind.Behind }} ↓{{ .AheadBehind.Behind }}{{ end }}{{ if .Untracked.Changed }} \uf059 {{ .Untracked.String }}{{ end }}]",
          "foreground": "p:pink",
          "type": "git",
          "style": "plain"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "python",
          "properties": {
            "display_mode": "files"
          },
          "template": "[{{ if not .Error }}\udb80\udf20 {{ if .Venv }}({{ .Venv }} ){{ end }}{{ .Full }}{{ end }}]",
          "foreground": "p:sapphire",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "<b>\udb80\udd3e</b> ",
          "foreground": "p:blue",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground_templates": [
      "{{ if gt .Code 0 }}p:red{{ else }}p:green{{ end }}"
    ],
    "template": "\udb80\udd3e "
  },
  "secondary_prompt": {
    "template": "\udb80\udd3e\u2800",
    "foreground": "p:blue",
    "background": "transparent"
  },

  "upgrade": {
    "auto": true,
    "source": "cdn",
    "interval": "168h"
  },
  "version": 3
}
