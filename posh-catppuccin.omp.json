{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "palette": {
        "bg_green": "#A7C080",
        "fg_green": "#425047",
        "aqua": "#83C092"
    },
    "console_title_template": "{{if .Root}}[root] {{end}}{{.Shell}} in <{{.Folder}}>",
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "foreground": "p:aqua",
                    "background": "p:fg_green",
                    "leading_diamond": "",
                    "template": "  ",
                    "style": "diamond",
                    "trailing_diamond": "",
                    "type": "text"
                },
                {
                    "foreground": "#425047",
                    "background": "#A7C080",
                    "leading_diamond": "",
                    "template": "{{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }}@{{ .HostName }}",
                    "style": "diamond",
                    "trailing_diamond": "",
                    "type": "session"
                },
                {
                    "background": "p:bg_green",
                    "foreground": "p:fg_green",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "template": "  {{ .Path }} ",
                    "properties": {
                        "style": "agnoster_short"
                    },
                    "style": "diamond",
                    "type": "path"
                },
                {
                    "properties": {
                        "cache_duration": "none",
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "fetch_upstream_icon": true
                    },
                    "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "background": "p:bg_green",
                    "foreground": "p:fg_green",
                    "type": "git",
                    "style": "diamond",
                    "background_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#D08770{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#EBCB8B{{ end }}",
                        "{{ if gt .Ahead 0 }}#B48EAD{{ end }}",
                        "{{ if gt .Behind 0 }}#B48EAD{{ end }}"
                    ]
                },
                {
                    "properties": {
                        "always_enabled": true,
                        "cache_duration": "none"
                    },
                    "trailing_diamond": "\ue0b0",
                    "template": " {{ if gt .Code 0 }}\uf00d {{ else }}\uf42e {{ end }}",
                    "foreground": "p:bg_green",
                    "background": "p:fg_green",
                    "type": "status",
                    "style": "diamond",
                    "background_templates": [
                        "{{ if gt .Code 0 }}#425047 {{ end }}"
                    ]
                }
            ],
            "newline": true
        },
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "type": "time",
                    "style": "diamond",
                    "foreground": "p:bg_green",
                    "template": "\ue641 {{.CurrentDate | date .Format }}"
                },
                {
                    "properties": {
                        "cache_duration": "none"
                    },
                    "template": "| \uf489 {{ .Name }}",
                    "foreground": "p:bg_green",
                    "type": "shell",
                    "style": "diamond"
                }
            ],
            "newline": true
        },
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "properties": {
                        "always_enabled": true,
                        "cache_duration": "none"
                    },
                    "template": "",
                    "foreground": "p:bg_green",
                    "background": "transparent",
                    "type": "status",
                    "style": "plain",
                    "foreground_templates": [
                        "{{if gt .Code 0}}#ff3030{{end}}"
                    ]
                }
            ],
            "newline": true
        }
    ],
    "version": 3,
    "final_space": true
}